        "## LANGKAH 2: PEMBERSIHAN DAN PERSIAPAN DATA (DATA PREPROCESSING)\n",
        "\n",
        "# 1. Tampilkan jumlah data sebelum dibersihkan\n",
        "print(f\"Jumlah data sebelum dibersihkan: {len(df)}\")\n",
        "\n",
        "# 2. Filter ulasan yang berbahasa Inggris ('en')\n",
        "df_en = df[df['lang'] == 'en'].copy()\n",
        "print(f\"Jumlah data setelah filter bahasa Inggris: {len(df_en)}\")\n",
        "\n",
        "# 3. Hapus baris data yang kosong pada kolom-kolom penting\n",
        "df_en.dropna(subset=['extract', 'score', 'product'], inplace=True)\n",
        "print(f\"Jumlah data setelah membuang baris kosong: {len(df_en)}\")\n",
        "\n",
        "# 4. Buat kolom 'brand' dari kolom 'product'\n",
        "# Mengambil kata pertama dari nama produk sebagai merek\n",
        "def get_brand(product_name):\n",
        "    if isinstance(product_name, str):\n",
        "        # Beberapa merek seperti \"Motorola\" atau \"Sony Ericsson\" bisa jadi dua kata\n",
        "        # Kita buat sedikit lebih pintar\n",
        "        brand = product_name.split()[0]\n",
        "        if brand.lower() == 'sony' and 'ericsson' in product_name.lower():\n",
        "            return 'Sony Ericsson'\n",
        "        return brand\n",
        "    return 'Unknown'\n",
        "\n",
        "df_en['brand'] = df_en['product'].apply(get_brand)\n",
        "\n",
        "# 5. Buat kolom 'sentiment' dari kolom 'score'\n",
        "# Skala skor dari 1-10\n",
        "def score_to_sentiment(score):\n",
        "    if score >= 7:\n",
        "        return 'Positif'\n",
        "    elif score >= 4:\n",
        "        return 'Netral'\n",
        "    else: # score 1, 2, 3\n",
        "        return 'Negatif'\n",
        "\n",
        "df_en['sentiment'] = df_en['score'].apply(score_to_sentiment)\n",
        "\n",
        "# 6. Pilih hanya kolom yang kita butuhkan untuk analisis selanjutnya\n",
        "kolom_penting = ['brand', 'product', 'sentiment', 'score', 'extract']\n",
        "df_bersih = df_en[kolom_penting].copy()\n",
        "\n",
        "\n",
        "# --- Tampilkan Hasil Akhir ---\n",
        "print(\"\\n--- HASIL DATA YANG SUDAH BERSIH ---\")\n",
        "print(f\"Total data yang siap dianalisis: {len(df_bersih)}\")\n",
        "\n",
        "print(\"\\nDistribusi Sentimen:\")\n",
        "print(df_bersih['sentiment'].value_counts())\n",
        "\n",
        "print(\"\\nDistribusi Merek Teratas:\")\n",
        "print(df_bersih['brand'].value_counts().head(10))\n",
        "\n",
        "print(\"\\nContoh Data Bersih:\")\n",
        "print(df_bersih.head())"

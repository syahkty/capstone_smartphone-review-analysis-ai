    {
      "cell_type": "code",
      "source": [
        "def classify_with_granite(review_text):\n",
        "    \"\"\"\n",
        "    Mengklasifikasikan teks ulasan menggunakan LLM Granite.\n",
        "    \"\"\"\n",
        "    # Membuat prompt lengkap dengan memasukkan teks ulasan\n",
        "    prompt = prompt_template.format(review_text=review_text)\n",
        "\n",
        "    try:\n",
        "        # Memanggil LLM untuk mendapatkan prediksi kategori\n",
        "        response = llm.invoke(prompt)\n",
        "\n",
        "        # Membersihkan respons, kadang ada spasi atau karakter tidak perlu\n",
        "        predicted_topic = response.strip()\n",
        "\n",
        "        # Validasi, jika output LLM aneh, anggap 'Lainnya'\n",
        "        valid_topics = ['Baterai', 'Layar', 'Kamera', 'Performa', 'Harga', 'Desain/Bodi', 'Software', 'Lainnya']\n",
        "        if predicted_topic not in valid_topics:\n",
        "            return 'Lainnya'\n",
        "\n",
        "        return predicted_topic\n",
        "\n",
        "    except Exception as e:\n",
        "        # Jika terjadi error saat memanggil API\n",
        "        print(f\"Terjadi error: {e}\")\n",
        "        return \"Gagal_Klasifikasi\""
      ],
